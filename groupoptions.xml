<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/group-options.xsd">
	<import>
		<categories>
			<category name="user.filebase">
				<parent>user</parent>
			</category>
			<category name="user.filebase.general">
				<parent>user.filebase</parent>
			</category>
			<category name="user.filebase.category">
				<parent>user.filebase</parent>
			</category>
			<category name="user.filebase.entry">
				<parent>user.filebase</parent>
			</category>
			<category name="user.filebase.entryComment">
				<parent>user.filebase</parent>
				<options>module_comment</options>
			</category>
			<category name="user.filebase.entryImage">
				<parent>user.filebase</parent>
			</category>
			<category name="user.filebase.entryFile">
				<parent>user.filebase</parent>
			</category>
			<category name="mod.filebase">
				<parent>mod</parent>
			</category>
			<category name="mod.filebase.entry">
				<parent>mod.filebase</parent>
			</category>
			<category name="mod.filebase.entryComment">
				<parent>mod.filebase</parent>
				<options>module_comment</options>
			</category>
			<category name="mod.filebase.entryImage">
				<parent>mod.filebase</parent>
			</category>
			<category name="mod.filebase.entryFile">
				<parent>mod.filebase</parent>
			</category>
			<category name="admin.filebase">
				<parent>admin.content</parent>
				<showorder>1</showorder>
			</category>
		</categories>

		<options>
			<!-- user.filebase.read -->
			<option name="user.filebase.canViewFilebaseOffline">
				<categoryname>user.filebase.general</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
			</option>
			<!-- /user.filebase.read -->

			<!-- user.filebase.category -->
			<option name="user.filebase.canViewCategory">
				<categoryname>user.filebase.category</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<option name="user.filebase.canEnterCategory">
				<categoryname>user.filebase.category</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<!-- /user.filebase.category -->

			<!-- user.filebase.entry -->
			<option name="user.filebase.canViewEntry">
				<categoryname>user.filebase.entry</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<option name="user.filebase.canViewOwnEntry">
				<categoryname>user.filebase.entry</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<option name="user.filebase.canAddEntry">
				<categoryname>user.filebase.entry</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<option name="user.filebase.canAddEntryWithoutModeration">
				<categoryname>user.filebase.entry</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<option name="user.filebase.canRateEntry">
				<categoryname>user.filebase.entry</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<option name="user.filebase.canEditOwnEntry">
				<categoryname>user.filebase.entry</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<option name="user.filebase.canDeleteOwnEntry">
				<categoryname>user.filebase.entry</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<option name="user.filebase.canSetEntryPrefix">
				<categoryname>user.filebase.entry</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<option name="user.filebase.canSetEntryTags">
				<categoryname>user.filebase.entry</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
				<options>module_tagging</options>
			</option>
			<!-- /user.filebase.entry -->

			<!-- user.filebase.entryComment -->
			<option name="user.filebase.canCommentEntry">
				<categoryname>user.filebase.entryComment</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<option name="user.filebase.canEditOwnEntryComment">
				<categoryname>user.filebase.entryComment</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
