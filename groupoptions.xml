				<categoryname>mod.filebase.entry</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
			</option>
			<option name="mod.filebase.canDeleteEntry">
				<categoryname>mod.filebase.entry</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
			</option>
			<option name="mod.filebase.canViewDeletedEntry">
				<categoryname>mod.filebase.entry</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
			</option>
			<option name="mod.filebase.canDeleteEntryCompletely">
				<categoryname>mod.filebase.entry</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
			</option>
			<option name="mod.filebase.canEnableEntry">
				<categoryname>mod.filebase.entry</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
			</option>
			<option name="mod.filebase.canMoveEntry">
				<categoryname>mod.filebase.entry</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
			</option>
			<!-- /mod.filebase.entry -->

			<!-- mod.filebase.entryComment -->
			<option name="mod.filebase.canEditEntryComment">
				<categoryname>mod.filebase.entryComment</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
			</option>
			<option name="mod.filebase.canDeleteEntryComment">
				<categoryname>mod.filebase.entryComment</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
