				<categoryname>mod.filebase.entryFile</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
			</option>
			<option name="mod.filebase.canDeleteEntryFile">
				<categoryname>mod.filebase.entryFile</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
			</option>
			<!-- /mod.filebase.entryFile -->

			<!-- admin.filebase -->
			<option name="admin.filebase.canAddCategory">
				<categoryname>admin.filebase</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
			</option>
			<option name="admin.filebase.canEditCategory">
				<categoryname>admin.filebase</categoryname>
				<optiontype>boolean</optiontype>
