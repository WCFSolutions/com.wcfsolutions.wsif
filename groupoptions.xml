			<option name="user.filebase.canDeleteOwnEntryComment">
				<categoryname>user.filebase.entryComment</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<option name="user.filebase.canDownloadEntryCommentAttachment">
				<categoryname>user.filebase.entryComment</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<option name="user.filebase.canViewEntryCommentAttachmentPreview">
				<categoryname>user.filebase.entryComment</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<option name="user.filebase.canUploadEntryCommentAttachment">
				<categoryname>user.filebase.entryComment</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
				<enableoptions>user.filebase.maxEntryCommentAttachmentSize,user.filebase.allowedEntryCommentAttachmentExtensions,user.filebase.maxEntryCommentAttachmentCount</enableoptions>
			</option>
			<option name="user.filebase.maxEntryCommentAttachmentSize">
				<categoryname>user.filebase.entryComment</categoryname>
				<optiontype>integer</optiontype>
				<defaultvalue>150000</defaultvalue>
			</option>
			<option name="user.filebase.allowedEntryCommentAttachmentExtensions">
				<categoryname>user.filebase.entryComment</categoryname>
				<optiontype>textarea</optiontype>
				<defaultvalue><![CDATA[gif
jpg
jpeg
png
bmp
zip
txt
pdf]]></defaultvalue>
			</option>
			<option name="user.filebase.maxEntryCommentAttachmentCount">
				<categoryname>user.filebase.entryComment</categoryname>
				<optiontype>integer</optiontype>
				<defaultvalue>5</defaultvalue>
			</option>
			<!-- /user.filebase.entryComment -->

			<!-- user.filebase.entryImage -->
			<option name="user.filebase.allowedEntryImageExtensions">
				<categoryname>user.filebase.entryImage</categoryname>
				<optiontype>textarea</optiontype>
				<defaultvalue><![CDATA[gif
jpg
jpeg
png
bmp]]></defaultvalue>
			</option>
			<option name="user.filebase.maxEntryImageSize">
				<categoryname>user.filebase.entryImage</categoryname>
				<optiontype>integer</optiontype>
				<defaultvalue>20000</defaultvalue>
			</option>
			<option name="user.filebase.maxEntryImageWidth">
				<categoryname>user.filebase.entryImage</categoryname>
				<optiontype>integer</optiontype>
				<defaultvalue>2000</defaultvalue>
			</option>
			<option name="user.filebase.maxEntryImageHeight">
				<categoryname>user.filebase.entryImage</categoryname>
				<optiontype>integer</optiontype>
				<defaultvalue>2000</defaultvalue>
			</option>
			<option name="user.filebase.maxImagesPerEntry">
				<categoryname>user.filebase.entryImage</categoryname>
				<optiontype>integer</optiontype>
				<defaultvalue>5</defaultvalue>
			</option>
			<option name="user.filebase.canEditOwnEntryImage">
				<categoryname>user.filebase.entryImage</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<option name="user.filebase.canDeleteOwnEntryImage">
				<categoryname>user.filebase.entryImage</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<!-- /user.filebase.entryImage -->

			<!-- user.filebase.entryFile -->
			<option name="user.filebase.canDownloadEntryFile">
				<categoryname>user.filebase.entryFile</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<option name="user.filebase.allowedEntryFileExtensions">
				<categoryname>user.filebase.entryFile</categoryname>
				<optiontype>textarea</optiontype>
				<defaultvalue><![CDATA[gif
jpg
jpeg
png
bmp
zip
txt
pdf]]></defaultvalue>
			</option>
