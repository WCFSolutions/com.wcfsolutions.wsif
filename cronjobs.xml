	<import>
		<cronjob>
			<classpath>lib/system/cronjob/CleanupCronjob.class.php</classpath>
			<description>Hourly Cleanup</description>
			<startminute>*/30</startminute>
			<starthour>*</starthour>
			<startdom>*</startdom>
